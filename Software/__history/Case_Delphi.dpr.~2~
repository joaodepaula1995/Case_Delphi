program Case_Delphi;

uses
  Vcl.Forms,
  unitLogin in 'unitLogin.pas' {FRM_LOGIN},
  unit_Fecha_Sistema in 'unit_Fecha_Sistema.pas' {FRM_FECHA_SISTEMA};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TFRM_LOGIN, FRM_LOGIN);
